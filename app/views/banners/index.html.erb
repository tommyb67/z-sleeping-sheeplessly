<h1>Banners</h1>

<table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>
              #
            </th>
            <th>
              Start Date
            </th>
            <th>
              End Date
            </th>
            <th>
              Image
            </th>
            <th>
              User
            </th>
          </tr>
        </thead>
        <tbody>
          <% @banners.each do |banner| %>
          <tr>
            <td>
              <%= banner.id %>
            </td>
            <td>
              <%= banner.start_date %>
            </td>
            <td>
              <%= banner.end_date %>
            </td>
            <td>
              <%= banner.image %>
            </td>
            <td>
              <%= banner.user.last_name %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
